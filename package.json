{
  "name": "fkn-media-player",
  "version": "0.0.2",
  "description": "",
  "type": "module",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "dev": "vite --port 1234",
    "build": "vite build && npm run copy-dependencies && tsc",
    "copy-dependencies": "npm run copy-libass && npm run copy-worker && npm run copy-worker-deps && npm run copy-worker-wasm",
    "copy-worker": "shx cp node_modules/@banou26/oz-libav/build/worker.js build/worker.js",
    "copy-worker-deps": "copyfiles -u 4 node_modules/@banou26/oz-libav/build/shared-memory-api_pb.*.js build",
    "copy-worker-wasm": "shx cp node_modules/@banou26/oz-libav/build/libav.wasm build/worker.wasm",
    "copy-libass": "copyfiles -u 4 ./node_modules/libass-wasm/dist/js/* build && shx rm build/COPYRIGHT"
  },
  "author": "Banou26",
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "concurrently": "^7.0.0",
    "copyfiles": "^2.4.1",
    "mime": "^3.0.0",
    "react-dom": "^18.2.0",
    "shx": "^0.3.4",
    "typescript": "^4.6.3",
    "vite": "^3.2.4"
  },
  "dependencies": {
    "@banou26/oz-libav": "^0.1.6-ci-3614283292.0",
    "@emotion/react": "^11.10.5",
    "libass-wasm": "^4.0.0",
    "mp4box": "^0.5.2",
    "osra": "^0.0.8",
    "p-queue": "^7.3.0",
    "react": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-tooltip": "^4.2.21"
  }
}
