{
  "name": "@banou/media-player",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "main": "build/index.js",
  "files": [
    "build",
    ".npmrc",
    "vite.config.ts",
    "tsconfig.json",
    "tsconfig.node.json",
    "src",
    "index.html"
  ],
  "scripts": {
    "dev": "vite --port 4560",
    "build": "vite build && npm run copy-dependencies && npm run types",
    "copy-dependencies": "npm run copy-libass && npm run copy-worker && npm run copy-worker-deps && npm run copy-worker-wasm",
    "copy-worker": "shx cp node_modules/@banou26/libav-wasm/build/worker.js build/worker.js",
    "copy-worker-deps": "copyfiles -u 4 node_modules/@banou26/libav-wasm/build/shared-memory-api_pb.*.js build",
    "copy-worker-wasm": "shx cp node_modules/@banou26/libav-wasm/build/libav.wasm build/libav.wasm",
    "copy-libass": "copyfiles -u 4 ./node_modules/libass-wasm/dist/js/* build",
    "types": "tsc"
  },
  "author": "Banou26",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "mime": "^3.0.0",
    "react-dom": "^18.2.0",
    "shx": "^0.3.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "dependencies": {
    "@banou26/libav-wasm": "^0.1.8-ci-3983365851.0",
    "@emotion/react": "^11.10.5",
    "libass-wasm": "^4.0.0",
    "mp4box": "^0.5.2",
    "osra": "^0.0.8",
    "p-queue": "^7.3.0",
    "react": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-tooltip": "^4.2.21"
  }
}
